{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn graphql:generate && yarn abis:generate && next dev",
    "build": "yarn graphql:generate && yarn abis:generate && next build",
    "vercel-build": "npm run build",
    "start": "next start",
    "ladle": "ladle serve",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,json}\"",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test-coverage": "yarn test --collect-coverage=true",
    "graphql:generate": "yarn graphql:generate:in-kind && yarn graphql:generate:uniswap && yarn graphql:generate:erc721",
    "graphql:generate:in-kind": "graphql-codegen --config codegen/inKind.yml",
    "graphql:generate:uniswap": "graphql-codegen --config codegen/uniswap.yml",
    "graphql:generate:erc721": "graphql-codegen --config codegen/erc721.yml",
    "abis:generate": "yarn typechain --target ethers-v5 --out-dir ./types/generated/abis './abis/*.json'"
  },
  "dependencies": {
    "@center-inc/react": "1.1.7",
    "@ethersproject/bignumber": "5.7.0",
    "@rainbow-me/rainbowkit": "0.9.0",
    "@react-hook/resize-observer": "1.2.6",
    "@sentry/nextjs": "7.37.2",
    "@uniswap/sdk-core": "3.1.1",
    "@uniswap/v3-core": "1.0.1",
    "@uniswap/v3-periphery": "1.4.3",
    "@uniswap/v3-sdk": "3.9.0",
    "@uniswap/widgets": "2.29.2",
    "@urql/core": "3.1.1",
    "axios": "0.26.1",
    "chart.js": "4.1.2",
    "chartjs-plugin-datalabels": "2.2.0",
    "d3": "^7.6.1",
    "datebook": "7.0.8",
    "dayjs": "1.11.0",
    "ethereumjs-util": "^7.1.5",
    "ethers": "5.7.2",
    "graphql": "16.3.0",
    "jotai-immer": "^0.1.0",
    "lambert-w-function": "^3.0.0",
    "lightweight-charts": "^3.8.0",
    "lodash": "4.17.21",
    "next": "^13.0.6",
    "normalize.css": "8.0.1",
    "numeral": "^2.0.6",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.28.1",
    "react-reveal": "^1.2.2",
    "react-select": "5.2.2",
    "react-slider": "^2.0.3",
    "reakit": "^1.3.11",
    "urql": "2.2.0",
    "wagmi": "0.10.6"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typed-document-node": "2.2.8",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-urql": "3.5.6",
    "@graphql-codegen/urql-introspection": "2.1.1",
    "@graphql-typed-document-node/core": "3.1.1",
    "@ladle/react": "^2.4.5",
    "@next/bundle-analyzer": "12.1.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@typechain/ethers-v5": "9.0.0",
    "@types/d3": "^7.4.0",
    "@types/lodash": "4.14.180",
    "@types/node": "17.0.22",
    "@types/numeral": "^2.0.2",
    "@types/react": "17.0.42",
    "@types/react-slider": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel-jest": "27.5.1",
    "eslint": "8.11.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "7",
    "jest": "27.5.1",
    "jest-fetch-mock": "3.0.3",
    "jsdom": "^20.0.0",
    "lint-staged": "12.3.7",
    "marked": "^4.0.17",
    "prettier": "2.6.0",
    "react-test-renderer": "18.2.0",
    "typechain": "7.0.1",
    "typescript": "4.9.4",
    "yarn": "1.22.18"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json}": [
      "prettier --write"
    ]
  }
}
